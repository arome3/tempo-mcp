# Tempo MCP Treasury Agent Example
# Copy this file to .env and fill in your values

# Required: Your wallet private key (0x-prefixed)
TEMPO_PRIVATE_KEY=0x...

# Optional: Network configuration (defaults to testnet)
TEMPO_RPC_URL=https://rpc.testnet.tempo.xyz
TEMPO_CHAIN_ID=42429

# Treasury Token Configuration
# Comma-separated list of tokens to track
TREASURY_TOKENS=AlphaUSD,AlphaEUR,AlphaGBP

# Target allocation percentages (must sum to 100)
TREASURY_TARGET_USD=60
TREASURY_TARGET_EUR=25
TREASURY_TARGET_GBP=15

# Rebalancing settings
TREASURY_DRIFT_THRESHOLD=5
TREASURY_MAX_SLIPPAGE=0.5
