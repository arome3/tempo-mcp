# Tempo MCP Payroll Agent Example
# Copy this file to .env and fill in your values

# Required: Your wallet private key (0x-prefixed)
TEMPO_PRIVATE_KEY=0x...

# Optional: Network configuration (defaults to testnet)
TEMPO_RPC_URL=https://rpc.testnet.tempo.xyz
TEMPO_CHAIN_ID=42429

# Payroll Configuration
TEMPO_DEFAULT_TOKEN=AlphaUSD

# Security Limits (optional, uses server defaults if not set)
TEMPO_MAX_BATCH_SIZE=100
TEMPO_MAX_SINGLE_PAYMENT=50000
TEMPO_DAILY_LIMIT=500000
