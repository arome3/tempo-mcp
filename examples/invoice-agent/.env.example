# Tempo MCP Invoice Agent Example
# Copy this file to .env and fill in your values

# Required: Your wallet private key (0x-prefixed)
TEMPO_PRIVATE_KEY=0x...

# Optional: Network configuration (defaults to testnet)
TEMPO_RPC_URL=https://rpc.testnet.tempo.xyz
TEMPO_CHAIN_ID=42429

# Invoice Configuration
TEMPO_DEFAULT_TOKEN=AlphaUSD

# Optional: Invoice settings
TEMPO_MEMO_PREFIX=INV-
TEMPO_RECONCILE_LOOKBACK=30
TEMPO_DUE_DATE_REMINDER=3
